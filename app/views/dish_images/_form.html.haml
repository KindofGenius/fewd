= form_for @dish_image do |f|
  - if @dish_image.errors.any?
    #error_explanation
      %h2= "#{pluralize(@dish_image.errors.count, "error")} prohibited this dish_image from being saved:"
      %ul
        - @dish_image.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :dish_id
    = f.number_field :dish_id
  .actions
    = f.submit 'Save'
