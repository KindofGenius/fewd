= form_for @restaurant_service do |f|
  - if @restaurant_service.errors.any?
    #error_explanation
      %h2= "#{pluralize(@restaurant_service.errors.count, "error")} prohibited this restaurant_service from being saved:"
      %ul
        - @restaurant_service.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :restaurant_id
    = f.number_field :restaurant_id
  .field
    = f.label :service_id
    = f.number_field :service_id
  .field
    = f.label :url
    = f.text_area :url
  .actions
    = f.submit 'Save'
